{
  "name": "alexeimun/onesignal",
  "description": "OneSignal Notifications driver",
  "homepage": "https://github.com/alexeimun/onesignal.git",
  "license": "MIT",
  "version": "1.3",
  "authors": [
    {
      "name": "Alexis Munoz",
      "email": "fact1128@hotmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "berkayk/onesignal-laravel": "^0.9.3",
    "illuminate/notifications": "5.*",
    "illuminate/support": "5.*"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.5",
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "OneSignalNotifier\\OneSignal\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OneSignalNotifier\\OneSignal\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "OneSignalNotifier\\OneSignal\\OneSignalServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
